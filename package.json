{
  "name": "on-the-radar",
  "version": "0.0.0",
  "description": "The website for the On the Radar study by the CSIS Project on Nuclear Issues (PONI) program.",
  "main": "gulpfile.js",
  "scripts": {
    "start": "gulp",
    "browsersync": "gulp browsersync",
    "browsersync-production": "gulp browsersync --jekyllEnv='production'",
    "build": "gulp build --jekyllEnv='production'",
    "netlify:build": "gulp build --jekyllEnv='production'",
    "eslint": "gulp eslint",
    "imagemin": "gulp imagemin",
    "production": "gulp --jekyllEnv='production'",
    "sass": "gulp sass",
    "watch": "gulp watch",
    "stylelint": "gulp stylelint",
    "stylelint-fix": "gulp stylelint --fix",
    "forestry:preview": "gulp --jekyllEnv='production' --forestry --preview --port 8080 --host 0.0.0.0",
    "forestry:publish": "gulp build --jekyllEnv='production' --forestry --port 8080 --host 0.0.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CSIS-iLab/csis-jekyll-starter.git"
  },
  "keywords": [
    "Autoprefixer",
    "Browsersync",
    "gulp",
    "imagemin",
    "Jekyll",
    "PostCSS",
    "Sass",
    "Webpack"
  ],
  "author": "CSIS iDeas Lab",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "browser-sync": "^2.26.7",
    "eslint-config-prettier": "^3.6.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-prettier": "^2.7.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-imagemin": "^6.2.0",
    "gulp-newer": "^1.4.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-stylelint": "^8.0.0",
    "gulp-watch": "^5.0.1",
    "imagemin-pngquant": "^6.0.1",
    "postcss-object-fit-images": "^1.1.2",
